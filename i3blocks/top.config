[window title]
command=xdotool getactivewindow getwindowname
interval=1
color=#A6D1FA

[bandwidth2]
full_text=loading bandwith
color=#46D1FA
command=$HOME/.config/i3blocks/bandwidth2/bandwidth2
interval=persist
markup=pango
LABEL=
INTERFACE=ens33

[disk]
color=#FFDC85
command=$HOME/.config/i3blocks/disk/disk
LABEL= 
DIR=/
ALERT_LOW=10
interval=30

[memory]
command=$HOME/.config/i3blocks/memory/memory
label= 
interval=30
color=#C3FA4A

[calendar]
command=$HOME/.config/i3blocks/calendar/calendar
interval=1
LABEL= 
DATEFMT=+%d.%m.%Y %H:%M
# SHORTFMT=+%H:%M:%S
HEIGHT=180
WIDTH=220
color=#FEFA4A

[temperature]
command=$HOME/.config/i3blocks/temperature/temperature
label= 
interval=10
color=#099A40
T_WARN=70
T_CRIT=90

[battery2]
command=$HOME/.config/i3blocks/battery2/battery2
markup=pango
interval=30
color=#12FA4A

[shutdown_menu]
full_text=
command=$HOME/.config/i3blocks/shutdown_menu/shutdown_menu
color=#FF4A4A
FG_COLOR=#bbbbbb
BG_COLOR=#111111
HLFG_COLOR=#111111
HLBG_COLOR=#bbbbbb
BORDER_COLOR=#222222
LOCKSCRIPT=i3lock --color=${BG_COLOR#"#"}
